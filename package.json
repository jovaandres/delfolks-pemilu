{
  "name": "cto-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node built/src/bin/www.js",
    "build": "tsc",
    "client:build": "cd client && npm install && npm run build",
    "dev-server": "SET NODE_ENV=development & nodemon --inspect built/src/bin/www.js",
    "client": "concurrently --kill-others-on-fail \"cd client && npm run serve\"",
    "dev-start": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run client\""
  },
  "dependencies": {
    "autoprefixer": "^10.3.6",
    "axios": "^0.22.0",
    "concurrently": "^6.3.0",
    "connect-history-api-fallback": "^1.6.0",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-enforces-ssl": "^1.1.0",
    "express-jwt": "^6.1.0",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "http-errors": "~1.8.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mongoose": "^6.0.8",
    "mongoose-simple-random": "^0.4.1",
    "mongoose-unique-validator": "^3.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.5.0",
    "passport-auth0": "^1.4.0",
    "passport-local": "^1.0.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.13",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "@types/axios": "^0.14.0",
    "@types/connect-history-api-fallback": "^1.3.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/crypto-js": "^4.0.2",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/helmet": "^4.0.0",
    "@types/http-errors": "^1.8.1",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/module-alias": "^2.0.1",
    "@types/mongoose-simple-random": "^0.4.9",
    "@types/mongoose-unique-validator": "^1.0.5",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.9",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34"
  },
  "_moduleAliases": {
    "@": "."
  }
}
